#!/usr/bin/env sh

# ğŸŒŒğŸŒ ğŸš€ Explore the Universe of Code ğŸš€ğŸŒ ğŸŒŒ

# Execute Husky hooks
. "$(dirname -- "$0")/_/husky.sh"

# Beautify your code with Prettier
yarn prettier:fix
yarn eslint .

# Check for lint warnings using ESLint
lint_output=$(yarn eslint .)

# No lint warnings? Code is a masterpiece!
if [ -z "$lint_output" ]; then
  echo "ğŸ‰ Your code is a work of art! No lint warnings found."
else
  # Show a gentle reminder for improvement
  cat << "EOF"

  ğŸ¨ Your code is almost perfect! Just a few brushstrokes needed. ğŸ¨




