#!/usr/bin/env sh

# Execute Husky hooks
. "$(dirname -- "$0")/_/husky.sh"

# Run Prettier to fix formatting
yarn prettier:fix
# yarn eslint .


# Check for lint warnings using ESLint
lint_output=$(yarn eslint .)

# Check if there are any lint warnings in the output
if echo "$lint_output" | grep -q "warning"; then
  echo "Warning: Project has error"
fi